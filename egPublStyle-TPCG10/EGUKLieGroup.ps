%!PS-Adobe-3.0 EPSF-3.0
%%Title: EG Letter logo (color)
%%BoundingBox: 39 697 126 756
%%HiResBoundingBox: 39.722057 696.417483 125.647937 755.597245
%%
% EG logo (colored or gray)
%
% without rescaling this logo will be at the size used on EG's
% official A4 letter stationary
%
/wid 0.07 def

/EG {     % def  scale x y color1 color2 -> -
    gsave
    /color2 exch def % define fill color values
    /color1 exch def
    0.00 setgray    % borders are black
    translate                  % position logo on page
    wid setlinewidth
    6.8 div dup  scale         % user  scale
    %
    % Letter E, top part
    newpath
    0 6.8 4.65 6.8 4.65 5.2 0 5.2 moveto lineto lineto lineto closepath
    gsave color1 aload pop setrgbcolor fill grestore
    stroke
    %
    % Letter E, bottom part
    newpath
    0 4.2 4.65 4.2 4.65 2.6 1.9 2.6 1.9 1.6 4.65 1.6 4.65 0 0 0 moveto
    lineto lineto lineto lineto lineto lineto lineto closepath
    gsave color2 aload pop setrgbcolor fill grestore
    stroke
    %
    5.25 0 translate          % position G relative to E
    %
    % Letter G, box part
    newpath
    3.4 4.2 5.5 4.2 5.5 2.6 3.4 2.6  moveto lineto lineto lineto closepath
    gsave color2 aload pop setrgbcolor fill grestore
    stroke
    %
    % Letter G, Curved part
    newpath
    5.5 6.8 moveto
    2.8 7 2.1 6.65 1.1 5.85 curveto
    0.25 5 0 4.25 0 3.4 curveto
    0 1.9 1.1 0.35 2.5 -0.115 curveto
    3.5 -0.45 4.45 -0.4 5.5 0.4 curveto
    5.5 2.4 lineto
    4.8 1.6 4.35 1.4 3.7 1.4 curveto
    3 1.4 2.475 1.9 2.155 2.55 curveto
    1.65 3.6 2.1 4.85 3.2 5.1 curveto
    4.2 5.35 4.8 5.2 5.5 5.2 curveto
    closepath
    gsave color1 aload pop setrgbcolor fill grestore
    stroke
    grestore
} def

% par's for EG:  
%  scale	the value 54 results in a width of 30 mm
%  bl_x bl_y	bottom left corner
%  col1 col2	colors for area 1 and 2 
%
/red1 [ 1 0.2 0.2 ] def
/red2 [ 0.8 0 0 ] def
54   40 700   red1 red2   EG

% /gray1 [ 0.8 0.8 0.8 ] def
% /gray2 [ 0.2 0.2 0.2 ] def
% 54   40 700   gray1 gray2   EG

showpage

